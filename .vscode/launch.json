{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "node",
            "request": "launch",
            "name": "Launch createCart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "createCart",
                "--path",
                "functions/create/create.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch deleteCart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "deleteCart",
                "--path",
                "functions/delete/delete.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch getCart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "getCart",
                "--path",
                "functions/get/get.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch add Item to cart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "addCartItem",
                "--path",
                "functions/cartItem/add/addCartItem.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch del Item on cart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "deleteCartItem",
                "--path",
                "functions/cartItem/delete/deleteCartItem.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Update Item on cart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "updateCartItem",
                "--path",
                "functions/cartItem/update/updateCartItem.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "checkout Cart",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "checkoutCart",
                "--path",
                "events/checkoutCart.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "checkout amazon pay session",
            "program": "${workspaceRoot}/node_modules/.bin/sls",
            "args": [
                "invoke",
                "local",
                "-f",
                "amazonpayNewSession",
                "--path",
                "functions/amazonpay/session.json"
            ]
        }
    ]
}